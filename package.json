{
  "name": "photobooth",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "less": "^2.7.2",
    "prop-types": "^15.6.0",
    "ramda": "^0.24.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build": "npm run build-less && npm run build-js",
    "build-less": "lessc src/index.less static/index.css --verbose",
